{
  "compilerOptions": {
    "outDir": "./dist/",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "strict": true,
    "sourceMap": true,
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": false,
    "noImplicitOverride": false,
    "allowSyntheticDefaultImports": true
  }
}
